{
    "matrix": {
        "homeserver": "https://example.com",
        "user_id": "@notflixbot:example.com",
        "device_name": "sample"
    },
    "notflixbot": {
        "radarr_url": "http://radarr",
        "radarr_api_key": "abc123",
        "themoviedb_api_key": "def456"
    },

    "cmd_prefixes": {
        "%ruok": "ruok",
        "%whoami": "whoami",
        "%key_sync": "key_sync"
    },
    "admin_cmds": [
        "%ruok"
    ],
    "admin_user_ids": [
        "@admin:example.com"
    ],

    "autotrust": false,

    "credentials_path": "credentials-sample.json",
    "storage_path": "/var/lib/notflixbot/store",

    "log": {
        "level": "INFO",
        "logfile": "notflixbot.log",
        "json": true,
        "stderr": true
    }
}
